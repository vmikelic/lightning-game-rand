-- _init()
function _init()

    explosions = {} -- initialize 40 explosion objects with random coordinates and random frame cycle
    for i=1,40 do
        add( explosions, {x=flr(rnd(128)),y=flr(rnd(128)),frame_cycle=flr(rnd(5))+3})
    end

end--_init()
